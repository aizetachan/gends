{
    "name": "gends",
    "version": "1.0.0",
    "description": "GenDS - StudioGen Design System",
    "metadata": {
        "author": "StudioGen Team",
        "repoURL": "https://github.com/studiogen/gends",
        "license": "MIT",
        "framework": "react"
    },
    "preview": {
        "mode": "esbuild",
        "theme": "dark",
        "background": "#09090b"
    },
    "aliases": {
        "@": "src",
        "@/utils": "src/utils",
        "@/components": "src/components",
        "@/styles": "src/styles"
    },
    "utilities": {
        "cn": {
            "path": "src/utils/cn.ts",
            "export": "cn",
            "stub": "function cn() { return Array.from(arguments).filter(Boolean).join(' '); }"
        }
    },
    "tokens": {
        "css": [
            "src/styles/globals.css",
            "src/styles/themes/dark.css",
            "src/styles/themes/light.css"
        ],
        "categories": {
            "colors": "--sg-color-",
            "spacing": "--sg-space-",
            "typography": "--sg-font-",
            "radius": "--sg-radius-",
            "shadows": "--sg-shadow-"
        }
    },
    "assets": {
        "icons": {
            "library": "lucide-react",
            "stub": true
        },
        "images": "public/images",
        "fonts": "public/fonts"
    },
