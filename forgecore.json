            "type": "input",
            "model": {
                "pattern": "controlledValue",
                "events": [
                    "onChange"
                ]
            },
            "variants": [
                {
                    "prop": "size",
                    "values": [
                        "sm",
                        "md",
                        "lg"
                    ],
                    "default": "md"
                }
            ],
            "defaultProps": {
                "label": "Dark mode",
                "size": "md"
            }
        },
        "ToolBar": {
            "entry": "src/components/ToolBar/ToolBar.tsx",
            "export": {
                "type": "named",
                "name": "ToolBar"
            },
            "styles": [
                "src/components/ToolBar/ToolBar.module.css"
            ],
            "dependencies": [
                "src/utils/cn.ts"
            ],
            "type": "layout",
            "defaultProps": {
                "children": "Toolbar Content"
            }
        },
        "Tooltip": {
            "entry": "src/components/Tooltip/Tooltip.tsx",
            "export": {
                "type": "named",
                "name": "Tooltip"
            },
            "styles": [
                "src/components/Tooltip/Tooltip.module.css"
            ],
            "dependencies": [
