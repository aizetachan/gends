{
    "name": "gends",
    "version": "1.0.0",
    "description": "GenDS - StudioGen Design System",
    "preview": {
        "mode": "esbuild",
        "theme": "dark"
    },
    "aliases": {
        "@": "src",
        "@/utils": "src/utils",
        "@/components": "src/components",
        "@/styles": "src/styles",
        "@/types": "src/types",
        "@/hooks": "src/hooks"
    },
    "utilities": {
        "cn": {
            "path": "src/utils/cn.ts",
            "export": "cn"
        }
    },
    "tokens": {
        "css": [
            "src/styles/globals.css",
            "src/styles/themes/dark.css",
            "src/styles/themes/light.css"
        ],
        "categories": {
            "spacing": "--sg-space-",
            "typography": "--sg-font-",
            "radius": "--sg-radius-",
            "shadows": "--sg-shadow-",
            "colors": "--sg-color-",
            "transitions": "--sg-transition-"
        }
    },
    "components": {
        "AlertDialog": {
            "entry": "src/components/AlertDialog/AlertDialog.tsx",
            "styles": "src/components/AlertDialog/AlertDialog.module.css",
            "dependencies": [
                "src/utils/cn.ts"
            ],
            "defaultProps": {
                "isOpen": true,
                "title": "Confirm Action",
                "description": "Are you sure you want to proceed?",
                "confirmText": "Confirm",
                "cancelText": "Cancel"
            }
        },
        "Avatar": {
            "entry": "src/components/Avatar/Avatar.tsx",
            "styles": "src/components/Avatar/Avatar.module.css",
            "dependencies": [
                "src/utils/cn.ts"
            ],
            "variants": [
                {
                    "prop": "size",
                    "values": [
                        "sm",
                        "md",
                        "lg"
                    ]
                }
            ],
            "defaultProps": {
                "name": "John Doe",
                "size": "md"
            }
        },
        "Badge": {
            "entry": "src/components/Badge/Badge.tsx",
            "styles": "src/components/Badge/Badge.module.css",
            "dependencies": [
                "src/utils/cn.ts"
            ],
            "variants": [
                {
                    "prop": "variant",
                    "values": [
                        "default",
                        "success",
                        "warning",
                        "error",
                        "info"
                    ]
                },
                {
                    "prop": "size",
                    "values": [
                        "sm",
                        "md",
                        "lg"
                    ]
                }
            ]
        },
        "Breadcrumb": {
            "entry": "src/components/Breadcrumb/Breadcrumb.tsx",
            "styles": "src/components/Breadcrumb/Breadcrumb.module.css",
            "dependencies": [
                "src/utils/cn.ts"
            ]
        },
        "Button": {
            "entry": "src/components/Button/Button.tsx",
            "styles": "src/components/Button/Button.module.css",
            "dependencies": [
                "src/utils/cn.ts"
            ],
            "variants": [
                {
                    "prop": "variant",
                    "values": [
                        "primary",
                        "secondary",
                        "ghost",
                        "danger",
                        "success"
                    ]
                },
                {
                    "prop": "size",
                    "values": [
                        "sm",
                        "md",
                        "lg"
                    ]
                }
            ],
            "defaultProps": {
                "variant": "primary",
                "size": "md",
                "children": "Button"
            }
        },
        "Card": {
            "entry": "src/components/Card/Card.tsx",
            "styles": "src/components/Card/Card.module.css",
            "dependencies": [
                "src/utils/cn.ts"
            ],
            "variants": [
                {
                    "prop": "variant",
                    "values": [
                        "default",
                        "elevated",
                        "outlined"
                    ]
                }
            ]
        },
        "Checkbox": {
            "entry": "src/components/Checkbox/Checkbox.tsx",
            "styles": "src/components/Checkbox/Checkbox.module.css",
            "dependencies": [
                "src/utils/cn.ts"
            ]
        },
        "ColorPicker": {
            "entry": "src/components/ColorPicker/ColorPicker.tsx",
