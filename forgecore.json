                "cancelLabel": "Cancel",
                "variant": "danger"
            },
            "forge": {
                "requiresPortal": true,
                "slots": {
                    "trigger": {
                        "kind": "Button",
                        "props": {
                            "children": "Open dialog"
                        }
                    }
                },
                "showcase": "<AlertDialog open={true} onClose={() => {}} title={props.title} description={props.description} confirmLabel={props.confirmLabel} cancelLabel={props.cancelLabel} variant={props.variant} />"
            }
