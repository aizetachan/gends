{
    "$schema": "https://forgecore.dev/schemas/forgecore.v1.json",
    "forgecoreVersion": "1.0.0",
    "name": "gends",
    "package": {
        "displayName": "GenDS - StudioGen Design System",
        "npmName": "gends",
        "version": "1.0.0",
        "license": "MIT",
        "repoURL": "https://github.com/aizetachan/gends.git",
        "homepage": "",
        "author": "StudioGen Team"
    },
    "metadata": {
        "framework": "react",
        "language": "typescript",
        "moduleFormat": "esm",
        "monorepo": false,
        "entryStrategy": "source",
        "notes": [
            "Este forgecore describe el repositorio a nivel source: entradas TSX, CSS Modules, tokens CSS, assets, y harness de interacci√≥n para preview sin Storybook."
        ]
    },
    "preview": {
        "mode": "esbuild",
        "theme": "dark",
        "background": "#09090b",
        "viewport": {
            "width": 1200,
            "height": 800,
            "deviceScaleFactor": 1
        },
        "behavior": {
            "disableAnimations": false,
            "preferReducedMotion": true,
            "logEvents": true,
            "captureConsole": true
        }
    },
    "runtime": {
        "react": {
            "version": "18.x"
        },
        "bundler": {
            "type": "esbuild",
            "platform": "browser",
            "jsx": "automatic",
            "tsconfigPath": "tsconfig.json",
            "define": {
                "process.env.NODE_ENV": "\"production\""
            }
        },
        "aliases": {
            "@": "src",
            "@/utils": "src/utils",
            "@/components": "src/components",
            "@/styles": "src/styles"
        },
        "globalCss": [
            "src/styles/globals.css",
            "src/styles/themes/dark.css",
            "src/styles/themes/light.css"
        ],
        "theme": {
            "strategy": "attribute",
            "attribute": "data-theme",
            "values": [
                "dark",
                "light"
            ],
            "default": "dark",
            "applyTo": "documentElement"
        },
        "mount": {
            "rootId": "forge-root",
            "wrapper": {
                "element": "div",
                "props": {
                    "style": "min-height:100vh; padding:24px; box-sizing:border-box;"
                }
            }
        },
        "portals": {
            "enabled": true,
            "rootId": "forge-portal-root",
            "attachTo": "body",
            "createIfMissing": true
        },
        "stubs": {
            "icons": {
                "library": "lucide-react",
                "enabled": true,
                "strategy": "component",
                "fallback": "span"
            }
        }
    },
    "utilities": {
        "cn": {
            "path": "src/utils/cn.ts",
            "export": {
                "type": "named",
                "name": "cn"
            },
            "stub": "function cn(){return Array.from(arguments).filter(Boolean).join(' ')}"
        }
    },
    "tokens": {
        "type": "css-variables",
        "css": [
            "src/styles/globals.css",
            "src/styles/themes/dark.css",
            "src/styles/themes/light.css"
        ],
        "categories": {
            "colors": "--sg-color-",
            "spacing": "--sg-space-",
            "typography": "--sg-font-",
            "radius": "--sg-radius-",
            "shadows": "--sg-shadow-"
        }
    },
    "assets": {
        "images": "public/images",
        "fonts": "public/fonts",
        "icons": {
            "library": "lucide-react",
            "stub": true
        }
    },
